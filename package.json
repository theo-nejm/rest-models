{
  "name": "rest-models",
  "author": {
    "name": "Antonio Sampaio"
  },
  "version": "1.0.7",
  "description": "Data manipulation made easy",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/rest-models/rest-models"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "lint": "eslint --fix 'src/**/*.ts'",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "mobx": "^6.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "rollup": "^2.56.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ]
  }
}
